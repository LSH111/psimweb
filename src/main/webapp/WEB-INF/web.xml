<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1">
    <display-name>PSIM Web Application</display-name>
    <filter>
        <filter-name>authFilter</filter-name>
        <filter-class>com.psim.web.cmm.filter.AuthFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>authFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <!-- 
    Spring Boot를 사용하면 거의 비워둬도 됩니다.
    Spring Boot가 모든 설정을 자동으로 처리하기 때문입니다.
    -->
</web-app>
